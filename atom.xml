---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Ajay Gupta</title>
 <link href="http://ajay-gupta.com/atom.xml" rel="self"/>
 <link href="http://ajay-gupta.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ajay-gupta.com</id>
 <author>
   <name>Ajay Gupta</name>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://ajay-gupta.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ajgupta.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
